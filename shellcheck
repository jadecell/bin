       [1mvim.statusline.lualine.activeSection.x[0m
           active config for: | A | B | C (X) | Y | Z |

           [4mType:[24m list of string

           [4mDefault:[0m

               [
                 ''
                   {
                     -- Lsp server name
                     function()
                       local buf_ft = vim.bo.filetype
                       local excluded_buf_ft = { toggleterm = true, NvimTree = true, ["neo-tree"] = true, TelescopePrompt = true }

                       if excluded_buf_ft[buf_ft] then
                         return ""
                         end

                       local bufnr = vim.api.nvim_get_current_buf()
                       local clients = vim.lsp.get_clients({ bufnr = bufnr })

                       if vim.tbl_isempty(clients) then
                         return "No Active LSP"
                       end

                       local active_clients = {}
                       for _, client in ipairs(clients) do
                         table.insert(active_clients, client.name)
                       end

                       return table.concat(active_clients, ", ")
                     end,
                     icon = 'ï‚… ',
                     separator = {left = 'î‚¾'},
                   }
                 ''
                 ''
                   {
                     "diagnostics",
                     sources = {'nvim_lsp', 'nvim_diagnostic', 'nvim_diagnostic', 'vim_lsp', 'coc'},
                     symbols = {error = 'ó°…™  ', warn = 'ï±  ', info = 'ïš  ', hint = 'ó°Œµ '},
                     colored = true,
                     update_in_insert = false,
                     always_visible = false,
                     diagnostics_color = {
                       color_error = { fg = 'red' },
                       color_warn = { fg = 'yellow' },
                       color_info = { fg = 'cyan' },
                     },
                   }
                 ''
               ]

           [4mDeclared[24m [4mby:[0m
               [1m<nvf/modules/plugins/statusline/lualine/lualine.nix>[0m

